<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="NiveauExperience" table="niveau_experience">
    <id name="idNiveauExperience" type="integer" column="id_niveau_experience">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="limiteHaute" type="integer" column="limite_haute" nullable="true"/>
    <field name="intitule" type="string" column="intitule" length="45" nullable="true"/>
    <field name="visuNiveau" type="string" column="visu_niveau" length="45" nullable="true"/>
    <field name="statut" type="string" column="statut" length="10" nullable="true"/>
    <field name="limiteBasse" type="integer" column="limite_basse" nullable="true"/>
  </entity>
</doctrine-mapping>
