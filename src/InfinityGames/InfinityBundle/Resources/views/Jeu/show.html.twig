{% extends "::administration/admin_layout.html.twig" %}
	{% block content_page %}
<h1>Jeu</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ entity.nom }}</td>
        </tr>
        <tr>
            <th>Note</th>
            <td>{{ entity.note }}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updateda</th>
            <td>{{ entity.updateda|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Visuelimg</th>
            <td>{{ entity.visuelImg }}</td>
        </tr>
        <tr>
            <th>Destindex</th>
            <td>{{ entity.destIndex }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('jeu') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('jeu_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('jeu_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
