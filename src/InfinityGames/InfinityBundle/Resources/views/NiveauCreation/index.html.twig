<h1>NiveauCreation list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Limitehaute</th>
            <th>Intitule</th>
            <th>Visuniveau</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('niveaucreation_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.limiteHaute }}</td>
            <td>{{ entity.intitule }}</td>
            <td>{{ entity.visuNiveau }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('niveaucreation_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('niveaucreation_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('niveaucreation_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
