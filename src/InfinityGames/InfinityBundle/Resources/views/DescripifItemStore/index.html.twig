<h1>DescripifItemStore list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Intitule</th>
            <th>Statut</th>
            <th>Descriptif</th>
            <th>Prix</th>
            <th>Dureetemps</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('descripifitemstore_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.intitule }}</td>
            <td>{{ entity.statut }}</td>
            <td>{{ entity.descriptif }}</td>
            <td>{{ entity.prix }}</td>
            <td>{{ entity.dureeTemps }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('descripifitemstore_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('descripifitemstore_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('descripifitemstore_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
