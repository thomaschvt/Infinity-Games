{% extends "::administration/admin_layout.html.twig" %}
	{% block content_page %}
<h1>GenreJeu list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Intitule</th>
            <th>Descriptif</th>
            <th>Visupicto</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('genrejeu_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.intitule }}</td>
            <td>{{ entity.descriptif }}</td>
            <td>{{ entity.visuPicto }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('genrejeu_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('genrejeu_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('genrejeu_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
	{% endblock %}