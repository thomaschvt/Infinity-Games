{% extends "::administration/admin_layout.html.twig" %}
	{% block content_page %}
		
		<div class="span1"></div>
		<div class="span11">
		<h2>Ajout d'un type de jeu</h2>
		<form action="{{ path('genrejeu_create') }}" method="post" {{ form_enctype(form) }}>
		    {{ form_widget(form) }}
		    <p>
		        <button class="btn btn-success" type="submit"><i class="icon-ok icon-white"></i> Créer</button>
		    </p>
		</form>
		<br/>
		<a href="{{ path('jeu') }}">
		            <button class="btn btn-inverse">
		            	<i class="icon-list icon-white"></i> Retour à la liste
		            </button>
		        </a>
		</div>
	{% endblock %}