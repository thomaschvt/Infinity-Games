<h1>LiaisonItemUser list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Item</th>
            <th>Utilisateur</th>
            <th>Date_achat</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('liaisonitemuser_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.item }}</td>
            <td>{{ entity.utilisateur }}</td>
            <td>{% if entity.dateachat %}{{ entity.dateachat|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('liaisonitemuser_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('liaisonitemuser_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('liaisonitemuser_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
